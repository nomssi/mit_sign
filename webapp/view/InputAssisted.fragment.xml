<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>searchHelp.Title}"
		class="sapUiPopupWithPadding"
		items="{helpView>/UserCollection}"
