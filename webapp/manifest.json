{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "Signature",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "${project.version}"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/mak/SIGNATURE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://signature",
			"favIcon": "img/favicon.ico",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "Signature.view.App",
			"type": "XML",
			"async": true,
			"id": "idAppControl"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Signature.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"useBatch": true,
					"defaultBindingMode": "OneWay"
				}
			}
		},
		"handleValidation": true,
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "Signature.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": "home"
				},
				{
					"pattern": "sign/{id}",
					"name": "sign",
					"target": "sign",
					"subroutes": []
				},
				{
					"name": "complete",
					"pattern": "complete/{id}",
					"target": "complete"
				},
				{
					"name": "blanko",
					"pattern": "blanko/{id}",
					"target": "blanko"
				},
				{
					"name": "error",
					"pattern": "error/:id:",
					"target": "error"
				}				
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1,
					"viewId": "appView",
					"title": "{i18n>appTitle}"
				},
				"sign": {
					"viewName": "Sign",
					"viewLevel": 2,
					"viewId": "signView"
				},
				"complete": {
					"viewName": "Complete",
					"viewLevel": 3,
					"viewId": "signComplete"
				},
				"error": {
					"viewName": "Error",
					"viewLevel": 3,
					"viewId": "signError"
				},
				"blanko": {
					"viewName": "Blanko",
					"viewLevel": 3,
					"viewId": "signBlanko"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound",
					"transition": "show"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}